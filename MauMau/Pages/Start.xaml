<Page x:Class="MauMau.Pages.Start"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MauMau.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="900" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
      Title="Start">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="{StaticResource Card_other_Wallpaper}" />
        </Grid.Background>
        <Button x:Name="btnplay" Style="{StaticResource RoundCorner}" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="332,0,338,80" Content="JOGAR!" FontFamily="Arial Bold" FontSize="30" Width="230" Height="62"/>
    </Grid>
</Page>
